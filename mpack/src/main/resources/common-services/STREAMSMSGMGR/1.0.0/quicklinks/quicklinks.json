{
    "name": "default",
    "description": "default quick links configuration",
    "configuration": {
        "protocol":
        {
          "type":"https",
          "checks":[
            {
              "property":"streams_messaging_manager.ssl.isenabled",
              "desired":"true",
              "site":"streams-messaging-manager-ssl-config"
            }
          ]
        },
        "links": [
            {
                "name": "streams_messaging_manager_ui",
                "label": "SMM Rest Swagger UI",
                "requires_user_name": "false",
                "component_name": "STREAMSMSGMGR",
                "url":"%@://%@:%@/swagger",
                "port":{
                    "http_property": "port",
                    "http_default_port": "8585",
                    "https_property": "streams_messaging_manager.ssl.port",
                    "https_default_port": "8587",
                    "regex": "^(\\d+)$",
                    "site": "streams-messaging-manager-common"
                }
            }
        ]
    }
}
