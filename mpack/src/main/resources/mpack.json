{
  "type": "full-release",
  "name": "smm-ambari-mpack",
  "version": "1.0.0.0",
  "description": "Ambari Management Pack for Streams Messaging Manager",
  "prerequisites": {
    "min-ambari-version": "2.7.0.0",
    "min-stack-versions": [
      {
        "stack_name": "HDF",
        "stack_version": "3.2.0.0"
      }
    ]
  },
  "artifacts": [
    {
      "name": "SMM-common-services",
      "type" : "service-definitions",
      "source_dir" : "common-services"
    },
    {
      "name" : "SMM-addon-services",
      "type" : "stack-addon-service-definitions",
      "source_dir": "addon-services",
      "service_versions_map": [
        {
          "service_name" : "STREAMSMSGMGR",
          "service_version" : "1.0.0",
          "applicable_stacks" : [
            {
              "stack_name" : "HDF",
              "stack_version" : "3.2"
            }
          ]
        }
      ]
    },
    {
      "name" : "SMM-addon-services",
      "type" : "stack-addon-service-definitions",
      "source_dir": "addon-services",
      "service_versions_map": [
        {
          "service_name" : "STREAMSMSGMGR",
          "service_version" : "1.0.0",
          "applicable_stacks" : [
            {
              "stack_name" : "HDP",
              "stack_version" : "3.0"
            }
          ]
        }
      ]
    }
  ]
}